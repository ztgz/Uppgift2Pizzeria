@model UserRoleViewModel

<table>
    <tr>
        <th>Namn</th>
        <th>Roll</th>
    </tr>
    <tr>
        <td>@Model.User.AnvandarNamn</td>
        <td>@Model.Role</td>
    </tr>
    
    <tr>
        <td colspan="2">
            <a class="btn btn-success" asp-controller="Account" asp-action="ChangeUserRole" 
               asp-route-userName="@Model.User.AnvandarNamn">
                Ändra roll
            </a>
        </td>
    </tr>
    
</table>

