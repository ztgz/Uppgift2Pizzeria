@model List<Bestallning>

<div class="row">
    <div class="col-12">
        <a class="btn btn-secondary mt-2 mr-2"
           asp-controller="Admin"
           asp-action="ListOfOrders"
           asp-route-delivered="@false"
           data-ajax="true"
           data-ajax-method="get"
           data-ajax-mode="append"
           data-ajax-update="#listOfOrders">Ej levereade ordrar</a>
        <a class="btn btn-secondary mt-2 mr-2"
           asp-controller="Admin"
           asp-action="ListOfOrders"
           asp-route-delivered="@true"
           data-ajax="true"
           data-ajax-method="get"
           data-ajax-mode="append"
           data-ajax-update="#listOfOrders">Levererade ordrar</a>
        <hr/>
    </div>
    <div class="col-5"></div>
</div>

<div class="row">
    <div class="col-12 col-sm-6" id="listOfOrders">
    </div>
    <div class="col-11 col-sm-5 overlayBox ml-2" id="infoBox"></div>
</div>
